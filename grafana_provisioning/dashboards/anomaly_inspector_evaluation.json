          "editorMode": "code",
          "format": "table",
          "hide": false,
          "rawQuery": true,
          "rawSql": "SELECT COUNT(a.time) AS false_positive\nFROM anomaly a\nWHERE a.topic = '$topic'\n  AND a.time >= $__timeFrom() AND a.time <= $__timeTo()\n  AND a.global = 1\n  AND a.time NOT IN (\n    SELECT l.time\n    FROM true_labels l\n    WHERE l.topic = '$topic'\n  )",

        {
          "dataset": "iox",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    time,\n    -- Absolute Error (AE)\n    ABS(online_pred - value) AS abs_error,\n \n    -- Cumulative Mean Absolute Error (MAE)\n    AVG(ABS(online_pred - value)) OVER (\n        PARTITION BY topic\n        ORDER BY time\n        ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW\n    ) AS mae_cummulative,\n \n    -- Squared Error (SE)\n    POWER(online_pred - value, 2) AS squared_error,\n \n    -- Cumulative RMSE\n    SQRT(\n        AVG(POWER(online_pred - value, 2)) OVER (\n            PARTITION BY topic\n            ORDER BY time\n            ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW\n        )\n    ) AS rmse_cummulative,\n \n    -- Cumulative Bias (mean signed error)\n    AVG(online_pred - value) OVER (\n        PARTITION BY topic\n        ORDER BY time\n        ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW\n    ) AS bias_cummulative\n \nFROM environment\nWHERE topic = '$topic' AND $__timeFilter(time)",
          "refId": "Performance metrics",
